@page
@model             EnggModel
@{
}


<div class="text-center">
    <h1 class="display-4">Engg Dept</h1>
    
    Welcome to the engg dept...
    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <td>Date</td>
                <td>Temp C</td>
                <td>Temp F</td>
                <td>Summary</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.WeatherForecastItems!)
            {
                <tr>
                    <td>@item.Date</td>
                    <td>@item.TemperatureC</td>
                    <td>@item.TemperatureF</td>
                    <td>@item.Summary</td>
                </tr>
            }
        </tbody>
    </table>    
</div>